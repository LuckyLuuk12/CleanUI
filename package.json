{
  "author": "Luuk Kablan",
  "name": "@kablan/clean-ui",
  "version": "1.0.8",
  "main": "css/index.css",
  "style": "css/index.css",
  "exports": {
    ".": {
      "sass": "./scss/index.scss",
      "style": "./css/index.css",
      "default": "./css/index.css"
