    "build": "sass scss/index.scss css/index.css --no-source-map"
  },
  "devDependencies": {
    "highlight.js": "^11.11.1",
    "sass": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuckyLuuk12/CleanUI.git"
  },
  "keywords": [
    "scss",
    "css",
    "ui",
    "design-system"
  ]
}
